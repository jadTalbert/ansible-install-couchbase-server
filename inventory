#this is the default inventory file
#for each node in your Couchbase cluster, simply repeat each host entry with the services you want running on the node
---
    couchbase_nodes:
      hosts:
        ec2-0-0-0-0.compute-1.amazonaws.com:
          services:
            - kv
        ec2-0-0-0-0.compute-1.amazonaws.com:
          services:
            - kv
            - n1ql
            - index
